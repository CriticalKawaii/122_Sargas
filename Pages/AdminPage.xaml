<Page x:Class="_122_Sargas.Pages.AdminPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:_122_Sargas.Pages"
      mc:Ignorable="d"
      d:Background="#FF9ACBBA"
      MinWidth="300"
      MaxWidth="300"
      Title="AdminPage">

    <StackPanel VerticalAlignment="Center">
        
        <Button x:Name="ButtonTab1e"
                Content="Таблица пользователей"
                Click="ButtonTab1e_Click"
                HorizontalAlignment="Stretch" />

        <Button x:Name="ButtonTab1e2"
                Content="Таблица категорий"
                Click="ButtonTab1e2_Click"
                HorizontalAlignment="Stretch" />

        <Button x:Name="ButtonTab1e3"
                Content="Таблица платежей"
                Click="ButtonTab1e3_Click"
                HorizontalAlignment="Stretch" />
        
        <Button x:Name="ButtonTab1e4"
                Content="Диаграммы"
                Click="ButtonTab1e4_Click"
                HorizontalAlignment="Stretch" />
    </StackPanel>
</Page>
